 #ifndef WINE_PRODUCTNAME_STR
-#define WINE_PRODUCTNAME_STR "Wine"
+#define WINE_PRODUCTNAME_STR "Microsoft® Windows® Operating System"
 #endif
 
 #ifndef WINE_EXTRAVALUES
@@ -111,7 +111,7 @@ FILEVERSION    WINE_FILEVERSION
